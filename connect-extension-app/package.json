{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ../detect-env-and-serve.js",
    "start:local": "ng serve --configuration=local --host localhost --port 5000 --disable-host-check --proxy-config proxy.conf.json",
    "start:replit": "ng serve --configuration=replit --host 0.0.0.0 --port 5000 --disable-host-check --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:local": "ng build --configuration=local",
    "build:local-extension": "ng build --configuration=local-extension",
    "build:replit": "ng build --configuration=replit",
    "build:prod": "ng build --configuration=production",
    "build:prod-extension": "ng build --configuration=prod-extension",
    "extension:local": "npm run build:local-extension && node ../generate-extension.js",
    "extension:prod": "npm run build:prod-extension && node ../generate-extension-prod.js",
    "replit": "npm run build:replit && mkdir -p ../connect-api/wwwroot && cp -r dist/* ../connect-api/wwwroot/",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.3.0",
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.14",
    "jasmine-core": "~4.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "~5.0.2"
  }
}
